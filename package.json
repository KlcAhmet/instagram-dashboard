    "build": "run-p build:*",
    "build:plasmo": "plasmo build",
    "build:next": "next build"
  },
  "dependencies": {
    "@plasmohq/redux-persist": "^6.1.0",
    "@reduxjs/toolkit": "^1.9.7",
    "cheerio": "^1.0.0-rc.12",
    "next": "13.4.19",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.1.3",
    "redux-persist-webextension-storage": "^1.0.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@plasmohq/rps": "1.8.7",
    "@plasmohq/storage": "1.8.1",
    "@types/chrome": "0.0.245",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/redux-persist-webextension-storage": "^1.0.3",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
