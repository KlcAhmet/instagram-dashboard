{
  "name": "instagram-dashboard",
  "displayName": "My Dashboard",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "ABK",
  "scripts": {
    "start": "next start",
    "dev": "run-p dev:*",
